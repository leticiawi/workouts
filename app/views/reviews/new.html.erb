<div class="container adjust-height" id="select">
  <h3>Rate your experience</h3>

  <div class="row">
    <div class="col-xs-4">
      <%= simple_form_for @review do |f| %>
        <%= f.input :rating, collection: (1..5).to_a %>
        <%= f.input :content %>
        <%= f.submit class: 'btn btn-info btn-block' "Send your review" %>
      <% end %>
    </div>
  </div>
</div>
